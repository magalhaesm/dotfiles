import atexit
import os
import readline
import rlcompleter

# history file
histfile = os.path.join(os.environ["XDG_CACHE_HOME"], "python_history")
try:
    readline.read_history_file(histfile)
except IOError:
    pass
atexit.register(readline.write_history_file, histfile)
del os, histfile, readline, rlcompleter

